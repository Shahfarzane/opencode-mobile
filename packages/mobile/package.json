{
  "name": "@opencode/mobile",
  "version": "1.2.0",
  "description": "OpenCode React Native mobile app",
  "private": true,
  "author": {
    "name": "Shahin Farzane",
    "email": "hey@nerd.ceo",
    "url": "https://github.com/Shahfarzane"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shahfarzane/opencode-mobile"
  },
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "ios": "expo run:ios",
    "ios:simulator": "expo start --ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "build:dev": "eas build --profile development --platform ios",
    "build:dev:simulator": "eas build --profile development-simulator --platform ios",
    "build:preview": "eas build --profile preview --platform ios",
    "build:production": "eas build --profile production --platform ios",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --config ../../eslint.config.js",
    "maestro": "source .maestro/.env && maestro test .maestro/flows",
    "maestro:studio": "source .maestro/.env && maestro studio",
    "maestro:record": "source .maestro/.env && maestro record",
    "maestro:ai": "source .maestro/.env && maestro test .maestro/flows/ai-explore.yaml"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.8",
    "@openchamber/shared": "workspace:*",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@ronradtke/react-native-markdown-display": "^8.1.0",
    "@shopify/flash-list": "2.0.2",
    "expo": "~54.0.30",
    "expo-blur": "^15.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-device": "^8.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "^17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~17.0.8",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.7.1",
    "uniwind": "^1.2.2",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@types/react": "~19.0.14",
    "babel-preset-expo": "^54.0.9",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3"
  }
}